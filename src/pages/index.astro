---import BaseLayout from "../layouts/BaseLayout.astro";import  AutoComplete  from "../components/Search/AutoComplete.tsx";import CohenLottery from '../images/CohenLottery.png';import DavidCohen from '../images/davidcohen.png';import ErnestineRose from '../images/ErnestineRose.jpg';import CharityBall from '../images/charityball.png';import FelixSaloon from '../images/felixsaloon.png';import LeviStrauss from '../images/LeviStrauss.png';import JonasLevy from '../images/JonasLevyPainting.jpg';import CohnStore from '../images/CohnStore.png';import PlayingCard from '../images/playingcard.png';import CanalLottery from '../images/CanalLottery.png';---<BaseLayout title="Home" /><div class="md:ml-32 grid place-items-center 2xl:grid-cols-2 xl:grid-cols-2 lg:grid-cols-2 md:grid-cols-2 sm:grid-cols-1 bg-white pr-4 pb-16 pl-4 mr-auto ml-auto  items-center relative lg:flex-row xl:py-8 mb-4">    <div class="col-span-1 mr-auto ml-auto max-w-xl xl:pr-32 lg:max-w-screen-xl">        <div class="lg:mb-0">            <div class="mb-6 max-w-xl">                <p class="inline-block pt-1 pr-3 pb-1 pl-3 mb-4 text-pink-200 bg-pink-500 rounded-2xl uppercase text-xs font-semibold tracking-wider">Beta</p>                <div class="text-3xl font-bold tracking-tight text-gray-900 max-w-lg sm:text-4xl sm:leading-none mb-6">                    <p class="text-gray-900 text-3xl font-bold tracking-tight sm:text-4xl sm:leading-none">Explore The Kaplan Collection</p>                </div>                <p class="text-gray-700 text-base md:text-lg">The Kaplan Collection documents commercial, social, religious, political, and cultural ties that connected Jews and the general public from the colonial era through the nineteenth century.</p>            </div>            <div class="flex flex-col md:flex-row"></div>            <div id="searchbox" class="flex items-center mt-4 mr-0 mb-0 ml-0">                <AutoComplete client:load />            </div>        </div>    </div>    <div class="lg:mb-15   md:col-span-1 mr-auto ml-auto max-w-xl xl:pr-32 lg:max-w-screen-xl">        <div class="grid grid-cols-4  sm:invisible md:visible md:grid-flow-row gap-4">            <!-- First Column -->            <div class="col-span-1 grid-flow-row-dense gap-2  ">                <div class="gap-2">                    <img src={CohenLottery.src} alt="Cohen Lottery Broadside" class="w-full h-auto rounded-lg object-cover"/>                </div>                <div class="gap-2">                    <img src={DavidCohen.src} alt="David Cohen Bar Ticket" class="w-full h-auto rounded-lg object-cover"/>                </div>                <div class="gap-2">                    <img src={ErnestineRose.src} alt="CDV of Ernestine Rose" class="w-full h-auto rounded-lg object-cover"/>                </div>            </div>            <!-- Second Column -->            <div class="col-span-1 grid-flow-row-dense gap-2">                <div class="gap-2">                    <img src={CharityBall.src} alt="Charity Ball Invitation" class="w-full h-auto rounded-lg object-cover"/>                </div>                <div class="gap-2">                    <img src={CanalLottery.src} alt="Canal Lottery Broadside" class="w-full h-auto rounded-lg object-cover"/>                </div>                <div class="gap-2">                    <img src={JonasLevy.src} alt="Painting of Jonas Levy's Ship" class="w-full h-auto rounded-lg object-cover"/>                </div>            </div>            <!-- Third Column -->            <div class="col-span-1 grid-flow-row-dense gap-2  ">                <div>                    <img src={FelixSaloon.src} alt="Felix Saloon Token" class="w-full h-auto rounded-lg object-cover"/>                </div>                <div>                    <img src={CohnStore.src} alt="CDV of Ernestine Rose" class="w-full h-auto rounded-lg object-cover"/>                </div>                <div>                    <img src={PlayingCard.src} alt="Samuel Hart Playing Cards" class="w-full h-auto rounded-lg object-cover"/>                </div>            </div>            <!-- Fourth Column -->            <div class="col-span-1 grid-flow-row-dense gap-2  ">                <div>                    <img src={LeviStrauss.src} alt="Levi Strauss Trade Card" class="w-full h-auto rounded-lg object-cover"/>                </div></div>        </div>    </div></div>