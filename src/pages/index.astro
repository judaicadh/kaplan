---import BaseLayout from "../layouts/BaseLayout.astro";import CohenLottery from '../images/CohenLottery.png';import DavidCohen from '../images/davidcohen.png';import Lichtenstein from '../images/Lichtenstein.png'import CharityBall from '../images/charityball.png';import FelixSaloon from '../images/felixsaloon.png';import LeviStrauss from '../images/LeviStrauss.png';import JonasLevy from '../images/JonasLevyPainting.jpg';import CohnStore from '../images/CohnStore.png';import PlayingCard from '../images/playingcard.png';import CanalLottery from '../images/CanalLottery.png';import Schlessel from '../images/schlessel.jpg'import NavAutocomplete from '../components/Search/NavAutocomplete'import WebsiteStructuredData from '@components/LinkedData/WebsiteStructuredData.astro'---<BaseLayout title="Home - Kaplan Collection of Early American Judaica"            description="The Kaplan Collection documents commercial, social, religious, political, and cultural ties that connected Jews and the general public from the colonial era through the nineteenth century.">    <section class="py-2 gap-2 place-items-start">        <div class="md:ml-32 grid grid-cols-1 md:grid-cols-2 bg-white pr-4 pl-4 mr-auto ml-auto relative xl:py-8 mb-4">            <!-- Text Content -->            <div class="self-start max-w-xl xl:pr-32 lg:max-w-screen-xl align-middle">                <p                  class="inline-block pt-1 pr-3 pb-1 pl-3 mb-4 text-pink-200 bg-pink-500 rounded-2xl uppercase text-xs font-semibold tracking-wider">                    Beta                </p>                <div class="text-3xl font-bold tracking-tight text-gray-900 max-w-lg sm:text-4xl sm:leading-none mb-6">                    <p>Discover The Kaplan Collection</p>                </div>                <p class="text-gray-700 text-base md:text-lg">                    The Kaplan Collection documents commercial, social, religious, political, and cultural ties that                    connected Jews                    and the general public from the colonial era through the nineteenth century.                </p>                <br />                <!-- Autocomplete Component -->                <div id="autocomplete">                    <NavAutocomplete client:only="reacct" />                </div>            </div>        <!-- Image Gallery -->        <div class="md:mr-32 pr-4 pl-4 invisible md:visible columns-3 md:columns-3 xl:columns-3 gap-7">            <div class="break-inside-avoid mb-8">                <div class="relative ">                    <a                      href="https://www.kaplancollection.org/item/lichtenstein-brothers-co-cigar-factory-advertising-broadside-c72d4fb3"                       class="absolute inset-0 z-10 bg-gray-200    text-center flex flex-col items-center justify-center opacity-0 hover:opacity-60 bg-opacity-90 duration-300">                        <h1 class="tracking-wider">Lichtenstein Brothers & Co. Cigar Factory Advertising Broadside</h1>                    </a>                    <a                      href="https://www.kaplancollection.org/item/lichtenstein-brothers-co-cigar-factory-advertising-broadside-c72d4fb3"                      class="relative">                        <img class="h-auto max-w-full rounded-lg" src={Lichtenstein.src} alt="Lichtenstein" />                    </a>                </div>            </div>            <div class="break-inside-avoid mb-8">                <div class="relative ">                    <a                      href="https://www.kaplancollection.org/item/broadside-advertisement-nathan-schlessel-fur-capes-new-ee4f608f"                       class="absolute inset-0 z-10 bg-gray-200    text-center flex flex-col items-center justify-center opacity-0 hover:opacity-60 bg-opacity-90 duration-300">                        <h1 class="tracking-wider">Broadside Advertisement for Nathan Schlessel, Fur Capes, New York, New                            York </h1>                    </a>                    <a                      href="https://www.kaplancollection.org/item/broadside-advertisement-nathan-schlessel-fur-capes-new-ee4f608f"                      class="relative">                        <img class="h-auto max-w-full rounded-lg" src={Schlessel.src} alt="Schlessel" />                    </a>                </div>            </div>            <div class="break-inside-avoid mb-8">                <div class="relative ">                    <a href="https://www.kaplancollection.org/item/felix-dining-saloon-civil-war-trade-token-4c083202"                       class="absolute inset-0 z-10 bg-gray-200    text-center flex flex-col items-center justify-center opacity-0 hover:opacity-60 bg-opacity-90 duration-300">                        <h1 class="tracking-wider">Felix Dining Saloon Civil War Trade Token</h1>                    </a>                    <a href="https://www.kaplancollection.org/item/felix-dining-saloon-civil-war-trade-token-4c083202"                       class="relative">                        <img class="h-auto max-w-full rounded-lg"                             src="https://images3-cdn.auctionmobility.com/is3/auctionmobility-static-cluster3/rLhw-1-11DHZ/3-U6IG1/f6c0dd73-a25b-4415-b917-89cff69b0dbe"                             alt="Felix Dining Saloon Civil War Trade Token" />                    </a>                </div>            </div>            <div class="break-inside-avoid mb-8">                <div class="relative ">                    <a                      href="https://www.kaplancollection.org/item/invitation-to-the-annual-hebrew-charity-ball-5c9da1b1"                       class="absolute inset-0 z-10 bg-gray-200    text-center flex flex-col items-center justify-center opacity-0 hover:opacity-60 bg-opacity-90 duration-300">                        <h1 class="tracking-wider">Invitation to the Annual Hebrew Charity Ball at the American Academy of                            Music, Philadelphia, February 6, 1873 </h1>                    </a>                    <a                      href="https://www.kaplancollection.org/item/invitation-to-the-annual-hebrew-charity-ball-5c9da1b1"                      class="relative">                        <img class="h-auto max-w-full rounded-lg" src={CharityBall.src} alt="Charity Ball" />                    </a>                </div>            </div>            <div class="break-inside-avoid mb-8">                <div class="relative ">                    <a                      href="https://www.kaplancollection.org/item/playing-cards-manufactured-by-samuel-hart-co-24c1c1b9"                       class="absolute inset-0 z-10 bg-gray-200    text-center flex flex-col items-center justify-center opacity-0 hover:opacity-60 bg-opacity-90 duration-300">                        <h1 class="tracking-wider">Playing Cards manufactured by Samuel Hart & Co., ca. 1860, New York,                            New York</h1>                    </a>                    <a                      href="https://www.kaplancollection.org/item/playing-cards-manufactured-by-samuel-hart-co-24c1c1b9"                      class="relative">                        <img class="h-auto max-w-full rounded-lg" src={PlayingCard.src} alt="Playing Card" />                    </a>                </div>            </div>            <div class="break-inside-avoid mb-8">                <div class="relative ">                    <a href="https://www.kaplancollection.org/item/trade-card-levi-strauss-co-seven-pocket-c815f850"                       class="absolute inset-0 z-10 bg-gray-200    text-center flex flex-col items-center justify-center opacity-0 hover:opacity-60 bg-opacity-90 duration-300">                        <h1 class="tracking-wider">Trade Card: Levi Strauss & Co. "Seven Pocket Engineers' Overall                            Riveted", San Francisco </h1>                    </a>                    <a href="https://www.kaplancollection.org/item/trade-card-levi-strauss-co-seven-pocket-c815f850"                       class="relative">                        <img class="h-auto max-w-full rounded-lg" src={LeviStrauss.src} alt="Levi Strauss" />                    </a>                </div>            </div>            <div class="break-inside-avoid mb-8">                <div class="relative ">                    <a                      href="https://www.kaplancollection.org/item/baltimore-hospital-lottery-broadside-a-cohens-lottery-421ef358"                       class="absolute inset-0 z-10 bg-gray-200    text-center flex flex-col items-center justify-center opacity-0 hover:opacity-60 bg-opacity-90 duration-300">                        <h1 class="tracking-wider">Baltimore Hospital Lottery Broadside from Cohen's Lottery & Exchange                            Office Advertisement, 1813</h1>                    </a>                    <a                      href="https://www.kaplancollection.org/item/baltimore-hospital-lottery-broadside-a-cohens-lottery-421ef358"                      class="relative">                        <img class="h-auto max-w-full rounded-lg" src={CohenLottery.src} alt="Cohen Lottery" />                    </a>                </div>            </div>        </div>    </div></section></BaseLayout>